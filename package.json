{
  "name": "inferno-firebase",
  "version": "0.1.0",
  "description": "Inferno bindings for Firebase",
  "author": "Magnus Bergman <hello@magnus.sexy> (http://github.com/magnus-bergman)",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src examples",
    "test": "node -r babel-register $(npm bin)/tape tests/**/*-test.js",
    "test:coverage": "node -r babel-register $(npm bin)/isparta cover $(npm bin)/tape -- src/**/*-test.js",
    "clean": "node -e 'console.log(require(\"./package\").files.join(\"\\n\"))' | xargs rm",
    "build": "babel src --ignore tests --out-dir .",
    "postbuild": "node -r babel-register scripts/build-umd.js",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "start": "node -r babel-register examples/server"
  },
  "unpkg": "umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/magnus-bergman/inferno-firebase.git"
  },
  "bugs": {
    "url": "https://github.com/magnus-bergman/inferno-firebase/issues"
  },
  "files": [
    "index.js",
    "Provider.js",
    "connect.js",
    "utils.js",
    "umd.js"
  ],
  "keywords": [
    "inferno",
    "infernojs",
    "firebase"
  ],
  "homepage": "https://github.com/magnus-bergman/inferno-firebase",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inferno": "^3.2.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-inferno": "^6.10.4",
    "express": "^4.15.2",
    "firebase": "^3.9.0",
    "inferno-test-utils": "^3.1.2",
    "isparta": "^4.0.0",
    "jsdom": "^10.1.0",
    "jsdom-global": "^3.0.2",
    "tape": "^4.6.3",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.10.2"
  },
  "dependencies": {
    "invariant": "^2.2.2",
    "prop-types": "^15.5.9"
  },
  "peerDependencies": {
    "firebase": "^3.9.0",
    "inferno": "^3.1.2",
    "inferno-component": "^3.1.2",
    "inferno-create-element": "^3.1.2"
  }
}
