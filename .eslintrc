{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "inferno",
    "jsx-a11y"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:inferno/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "Inferno" }]
  }
}
